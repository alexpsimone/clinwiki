{
  "extends": ["react-app", "eslint-config-airbnb"],
  "rules": {
    "semi": 2,
    "ter-arrow-parens": [2, "as-needed"]
  },
  "overrides": [
    {
      "files": [
        "**/*.ts?(x)"
      ],
      "rules": {
        "array-callback-return": "off",
        "eqeqeq": "off"
      }
    }
  ]
}
