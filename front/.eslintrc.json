{
  "extends": "eslint-config-airbnb",
  "rules": {
    "import-name": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "ter-arrow-parens": [true, "as-needed"],
    "max-line-length": [true, 120]
  }
}
